  "bugs": {
    "url": "https://github.com/msjanellej/Group_Capstone_Proj/issues"
  },
  "homepage": "https://github.com/msjanellej/Group_Capstone_Proj#readme",
  "devDependencies": {
    "autoprefixer": "^10.0.1",
    "concat": "^1.0.3",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.1.6",
    "postcss-cli": "^8.2.0"
  },
  "dependencies": {
    "jquery": "^3.5.1"
  }
}
